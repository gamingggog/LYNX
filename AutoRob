			local car = Workspace.Vehicles[plr.Name]
			local currentPos = car.Body.Mass.Position + Vector3.new(0,200,0)
			tweenTo(currentPos)
			tweenTo(safePos)
			clickAtCoordinates(0.5, 0.9)
