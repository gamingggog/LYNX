		local wantedRemoteEvent = game:GetService("ReplicatedStorage")["3ZJ"]["731e1826-fa12-4818-8787-356435fe206d"]
		local robRemoteEvent = game:GetService("ReplicatedStorage")["3ZJ"]["d826b47c-2853-4585-b67f-804febe691ac"]
